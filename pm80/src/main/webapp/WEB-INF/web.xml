		<servlet-class>com.ppumting.pm80.note.servlet.NoteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NoteServlet</servlet-name>
		<url-pattern>/pm80/add_note.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>pm80/sendnote</servlet-name>
		<url-pattern>/pm80/sendnote</url-pattern>
	</servlet-mapping></web-app>
