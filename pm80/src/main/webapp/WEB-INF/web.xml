<web-app>
	<servlet>
		<servlet-name>NoteServlet</servlet-name>
		<display-name>NoteServlet</display-name>
		<description></description>
		<servlet-class>com.ppumting.pm80.note.servlet.NoteServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>pm80/sendnote</servlet-name>
