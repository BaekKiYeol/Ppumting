<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app id="WebApp_ID">
	<display-name>pm80</display-name>
	<servlet>
		<servlet-name>CreateAccountServlet</servlet-name>
		<display-name>CreateAccountServlet</display-name>
		<description></description>
		<servlet-class>com.ppumting.pm80.point.servlet.CreateAccountServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>QnaFindServlet</servlet-name>
		<display-name>QnaFindServlet</display-name>
		<description></description>
		<servlet-class>com.ppumting.pm80.qna.servlet.QnaFindServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateAccountServlet</servlet-name>
		<url-pattern>/CreateAccountServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>QnaFindServlet</servlet-name>
		<url-pattern>/QnaFindServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>QnaFindServlet</servlet-name>
		<url-pattern>/Qna/qnaFind.do</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>
